datetime-format "%Y-%m-%d"

bind-key h quit
bind-key j down
bind-key k up
bind-key l open
bind-key g home
bind-key G end
bind-key S save-all
bind-key ^p halfpageup
bind-key ^n halfpagedown
bind-key m mark-feed-read

save-path ~/Documents/newsboat

browser "w3m %u"
reload-threads 100
cleanup-on-quit yes
